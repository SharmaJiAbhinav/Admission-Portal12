        <!-- End Detail -->

        <div class="album py-5">
            <div class="container">

                <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3">
                    <!-- card-1 -->
                    <div class="col">
                        <div class="card shadow-sm">
                            <img class="rounded w-100" src="./Images/Course-1.jpeg" alt="Course-1" height="200rem">
                            <div class="card-body text-center">
                                <h2>B.Tech</h2>
                                <p class="card-text">B.Tech is a professional undergraduate engineering degree</p>
                                <% if(btech) { %>
                                   <a href="" class="btn btn-info">Course Display</a> 
                                <% }else{ %>
                                    <button type="button" data-bs-target="#aa" data-bs-toggle="modal" class="btn btn-info text-white">Register</button>
                               <% } %> 
                               
                            </div>
                        </div>
                    </div>

                    <!-- card-2 -->
                    <div class="col">
                        <div class="card shadow-sm">
                            <img class="rounded w-100" src="./Images/Course-2.jpeg" alt="Course-2" height="200rem">
                            <div class="card-body text-center">
                                <h2>BCA</h2>
                                <p class="card-text">BCA is a three-year Bachelor's Degree in Computer Application</p>
                                <% if(bca) { %>
                                    <a href="" class="btn btn-info">Course Display</a> 
                                 <% }else{ %>
                                    <button type="button" data-bs-target="#ab" data-bs-toggle="modal" class="btn btn-info text-white">Register</button>
                                <% } %> 
                                
                                
                            </div>
                        </div>
                    </div>

                    <!-- card-3 -->
                    <div class="col">
                        <div class="card shadow-sm">
                            <img class="rounded w-100" src="./Images/Course-3.jpeg" alt="Course-3" height="200rem">
                            <div class="card-body text-center">
                                <h2>MCA</h2>
                                <p class="card-text">MCA is a professional post-graduation degree in Computer
                                    Application</p>
                                    <% if(mca) { %>
                                        <a href="" class="btn btn-info">Course Display</a> 
                                     <% }else{ %>
                                        <button type="button" data-bs-target="#abc" data-bs-toggle="modal" class="btn btn-info text-white">Register</button>
                                    <% } %> 
                                    
                                
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </section>
    <!-- End Courses -->

    <div class="modal fade" id="aa">
        <div class="modal-dialog mode-sm"> <!--modal-xl,sm,lg-->
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="model-title">login form</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal"></button>

                </div><!--modal header end-->
                <div class="modal-body">
                    <form action="/courseInsert" method="post" enctype="multipart/form-data">
                        <div class="mb-3">
                            Name
                            <input type="text" name="name" value="<%= n%>" placeholder="Enter Your Name" class="form-control">
                        </div>
                        <div class="mb-3">
                            Email
                            <input type="text" name="email" value="<%= e%>"  placeholder="Enter your Email" class="form-control">
                        </div>
                        <div class="mb-3">
                            Phone
                            <input type="text" name="phone"  placeholder="Contact Number" class="form-control">
                        </div>
                        <div class="mb-3">
                            D.O.B.
                            <input type="date" name="dob" placeholder="date of birth" class="form-control">
                        </div>
                        <div class="mb-3">
                            Address
                            <input type="text" name="address" placeholder="Address" class="form-control">
                        </div>
                        <div class="mb-3">
                            Gender
                            <input type="radio" name="gender" class="mr-2" value="Male">Male
                             <input type="radio" name="gender" class="mr-2" value="Female">Female
                        </div>
                        <div class="mb-3">
                            Qualification
                            <select name="qualification"  class="form-control">
                                <option value="">Qualification</option>
                                <option value="">10th</option>
                                <option value="">12th in Mathematics</option>
                            </select>
                            
                        </div>
                        <div class="mb-3">
                            Course
                            <input type="text" readonly name="Course" value="btech" class="form-control">
                        </div>
                        <button class="btn btn-info">Submit</button>
                    </form>

                </div><!--modal body-->
            </div><!--modal-content-->
        </div><!--modal dialog end-->
    </div><!--modal end-->

